{
	"manifest_version" : 2,
	"name" : "smart tabs",
	"description" : "This extension shows a Google Image search result for the current page",
	"version" : "1.0",
	"browser_action" : {
		"default_icon" : "icon.png",
		"default_popup" : "popup.html",
		"default_title" : "Click here!"
	},
	"permissions" : [
		"tabs",
		"activeTab",
		"storage",
		"idle",
		"tabCapture",
		"desktopCapture"
	],
	"background" : {
		"page": "background.html"
	},
	"options_page" : "options.html"
}